{% if menu %}
	<ul class="nav-main__items">
	{% for item in items %}
		<li class="{{ item.classes | join(' ') }} nav-main__item">
			<a target="{{ item.target }}" href="{{ item.link }}">{{ item.title }}</a>
			{% include "menu.twig" with {'items': item.children} %}
		</li>
	{% endfor %}
	</ul>
{% endif %}
